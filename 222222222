# Assuming config_data is a list of dictionaries
config_data = [
    # ... Your list of dictionaries
]

# Initialize an empty list to hold all bronze layer data
bronze_data_list = []

# Loop through each item in the list
for item in config_data:
    # Check if the item is a dictionary and has a key 'layer' with value 'bronze'
    if isinstance(item, dict) and item.get('layer') == 'bronze':
        bronze_data_list.append(item)

# Now bronze_data_list contains all the bronze data
print("Bronze Data:", bronze_data_list)
